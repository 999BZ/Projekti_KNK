<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginController">
   <children>
      <Label layoutX="160.0" layoutY="145.0" text="Username:" />
      <Label layoutX="162.0" layoutY="189.0" text="Password:" />
      <TextField fx:id="txtEmail" layoutX="222.0" layoutY="141.0" />
      <Button layoutX="320.0" layoutY="233.0" mnemonicParsing="false" onAction="#cancelClick" text="Cancel" />
      <PasswordField fx:id="pwdPassword" layoutX="223.0" layoutY="185.0" />
      <Button fx:id="lognBtn" layoutX="223.0" layoutY="233.0" mnemonicParsing="false" onAction="#loginClick" text="Log in" />
   </children>
</AnchorPane>
